<?xml version="1.0" encoding="UTF-8"?>
<!-- DTD 선언 마이바티스 설정파일임을 명시해주고 namespace에 맵퍼 파일 경로를 설정 -->
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.bebe.spring.user.dao.userInfoDAO">
	
   <update id="modify" parameterType="UsersVO">
   		update users
   		set passwd = #{passwd}
   		where id = 'aaa'
   </update>
    	
</mapper>